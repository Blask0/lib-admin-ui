{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2018", "es2017", "es2016", "es2015", "dom"],
    "outDir": "build/resources/main/assets/admin/common/js/",
    "sourceMap": false,
    "declaration": true,
    "declarationMap": false,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": false
  },
  "files": [
    "src/main/resources/assets/admin/common/js/entry.ts"
  ],
  "exclude": [
    ".vscode",
    "build/**/*"
  ],
  "types": [
    "types"
  ],
  "compileOnSave": false,
  "buildOnSave": false
}
